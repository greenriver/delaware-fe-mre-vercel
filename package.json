{
  "name": "delaware-ephtn-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "(cross-env ANALYZE=true next build && ls -a && http-server .next/analyze/ --port 8888)",
    "build_and_start": "(next build && next start)",
    "build-storybook": "storybook build -o docs-build -s ./public",
    "build": "next build",
    "check-formatting": "prettier -c 'src/**/*.{ts,tsx}'",
    "check-types": "tsc --noEmit",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
    "codegen": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config --config codegen.yml",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "fix-formatting": "prettier -w 'src/**/*.{ts,tsx}'",
    "fix-file-formatting": "prettier -w",
    "lint": "eslint --max-warnings 0 --ext ts --ext tsx ./src/",
    "lint-fix": "eslint --fix --max-warnings 0 --ext ts --ext tsx ./src/",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "storybook dev -p 6006 -s ./public",
    "storybook-test": "test-storybook --url http://dephtn-storybook.dev.test --watchAll",
    "tests": "ENVIRONMENT=test jest --json --outputFile=.jest-test-results.json --verbose",
    "tests-without-results": "ENVIRONMENT=test jest --verbose"
  },
